{
    "manifest_version": 3,
    "version": "0.0.1",
    "name": " messaging basic usage",
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content_script.js"
            ]
        }
    ],
    "permissions": [
        "tabs"
    ],
    "background": {
        "service_worker": "bg.js"
    }
}