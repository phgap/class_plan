{
  "name": "hello-extensions",
  "displayName": "hello-extensions",
  "description": "first extensions demo",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:hello-extensions.helloWorld",
    "onDebug"
  ],
